#!/bin/sh
#200000
./train_word2vec.py \
    --gpu 0 \
    --input_path /home/ubuntu/data/word2vec/20160920.wiki \
    --index2word_path /home/ubuntu/data/word2vec/20160920_index2word.pkl \
    --word2index_path /home/ubuntu/data/word2vec/20160920_word2index.pkl \
    --counts_path /home/ubuntu/data/word2vec/20160920_counts.pkl \
    --train_max_path /home/ubuntu/data/word2vec/20160920_train_max.pkl \
    --total_size_path /home/ubuntu/data/word2vec/20160920_total_size.pkl \
    --unit 200 \
    --batchsize 160000 \
    --out-type ns \
    --model cbow \
    --window 8 \
    --out /home/ubuntu/results/word2vec \
    --epoch 1 

