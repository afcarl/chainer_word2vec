#!/bin/sh
#200000
./train_word2vec.py \
    --gpu 0 \
    --input /home/ubuntu/data/word2vec/original_with_small_vocabulary/jawiki-wakati-index-sequence.txt \
    --index2word /home/ubuntu/data/word2vec/small/index2word.pkl \
    --word2index /home/ubuntu/data/word2vec/small/word2index.pkl \
    --unit 200 \
    --batchsize 160000 \
    --out-type ns \
    --model cbow \
    --window 8 \
    --out /home/ubuntu/data/word2vec/original_with_small_vocabulary/result \
    --epoch 5 

